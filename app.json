{
  "expo": {
    "name": "Agenda",
    "description": "A fast and simple agenda with a modern design. Build specifically for students and busy people.",
    "slug": "rocket-agenda",
    "githubUrl": "https://github.com/amargol/Rocket-Agenda",
    "primaryColor": "#333248",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#333248"
    },
    "androidStatusBar": {
      "backgroundColor": "#333248"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amargol.rocketagenda"
    },
    "android": {
      "package": "com.amargol.rocketagenda"
    }
  }
}
