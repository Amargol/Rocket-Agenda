{
  "expo": {
    "name": "Agenda",
    "description": "A fast and simple agenda with a modern design. Build specifically for students and busy people.",
    "slug": "rocket-agenda",
    "githubUrl": "https://github.com/amargol/Rocket-Agenda",
    "primaryColor": "#333248",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "5.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon_without_alpha_cropped.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "backgroundColor": "#333248"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amargol.rocketagenda",
      "buildNumber": "5.0.5"
    },
    "android": {
      "package": "com.amargol.rocketagenda",
      "versionCode": 5,
      "useNextNotificationsApi": true
    }
  }
}
